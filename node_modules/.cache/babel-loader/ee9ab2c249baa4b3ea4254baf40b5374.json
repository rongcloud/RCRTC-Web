AAA,EAAA,CAAA,EAAA,MAAA,CAAA,OAAA;AACA,WAAA,KAAA,CAAA,aAAA,GAAA,MAAA,CAAA,OAAA,GAAA,CAAA,CAAA,IAAA;AACA,KAVA;AAWA,IAAA,QAAA,EAAA,kBAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,CAAA;AACA,WAAA,KAAA,CAAA,QAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KAfA;AAgBA,IAAA,QAAA,EAAA,oBAAA;AACA;AACA,UAAA,KAAA,KAAA,CAAA,QAAA,IAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA;AACA;;AACA,UAAA,KAAA,KAAA,CAAA,KAAA,IAAA,GAAA,EAAA;AACA,aAAA,KAAA,CAAA,SAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,QADA;AAEA,UAAA,KAAA,EAAA,aAFA;AAGA,UAAA,WAAA,EAAA;AAHA,SAAA;AAKA,OANA,MAMA;AACA,aAAA,SAAA;AACA;AACA,KA/BA;AAgCA,IAAA,SAAA,EAAA,qBAAA;AAAA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,KAAA,CAAA,QADA;AAEA,QAAA,SAAA,EAAA,KAAA,KAAA,CAAA,KAFA;AAGA,QAAA,eAAA,EAAA,KAAA,KAAA,CAAA,aAHA;AAIA,QAAA,aAAA,EAAA,EAJA;AAKA,QAAA,QAAA,EAAA,CALA,CAKA;;AALA,OAAA;;AAOA,UAAA,KAAA,KAAA,CAAA,KAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,QAAA,GAAA,KAAA,KAAA,CAAA,QAAA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,MAAA,CAAA,OAAA,EAAA,KAAA,KAAA,CAAA,aAAA;AACA,MAAA,OAAA,CAAA,aAAA,CAAA,IAAA,EAAA,IAAA;AAAA,2EAAA,iBAAA,GAAA;AAAA;AAAA;AAAA;AAAA;AACA,sBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,KAAA,EAAA;AACA;AACA,oBAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EAAA;AACA,sBAAA,KAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AADA,qBAAA;;AAGA,oBAAA,KAAA,CAAA,KAAA,CAAA,WAAA,EAAA,GAAA,CAAA,IAAA;AACA,mBANA,MAMA;AACA,oBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,GAAA,CAAA,IAAA;AACA,oBAAA,OAAA,CAAA,QAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA;;AACA,oBAAA,KAAA,CAAA,KAAA,CAAA,WAAA,EAAA,GAAA,CAAA,IAAA;AACA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAaA;AAzDA;AAnBA,CAAA","sourcesContent":["<template>\n  <div class=\"createPanel\">\n    <div class=\"backIcon\" @click=\"back\"></div>\n    <div class=\"createContainer\">\n      <el-upload\n        class=\"titleIcon\"\n        :show-file-list=\"false\"\n        :action=\"updateImgUrl\"\n        :headers=\"{\n          authorization: this.$store.state.userInfo.authorization,\n          BusinessToken: btoken,\n        }\"\n        :on-success=\"handleAvatarSuccess\"\n        :before-upload=\"beforeAvatarUpload\"\n      >\n        <img v-if=\"imgBackground\" :src=\"imgBackground\" class=\"avatar\" />\n        <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n        <img :src=\"imgIcon\" class=\"avaterIcon\" />\n      </el-upload>\n\n      <div class=\"title\">\n        <input class=\"inputer\" v-model=\"roomName\" type=\"text\" maxlength=\"20\" />\n      </div>\n      <div class=\"check\">\n        <el-radio v-model=\"radio\" label=\"0\">公开 </el-radio>\n        <el-radio v-model=\"radio\" label=\"1\">私密 </el-radio>\n      </div>\n    </div>\n    <PopuInput ref=\"PopuInput\" @clickYes=\"clickYes\" />\n    <div class=\"startBtn\" @click=\"preStart\">开始视频直播</div>\n  </div>\n</template>\n<script>\nimport Request from \"../../request/index.js\";\nimport { btoken } from \"../../request/BusinessToken\";\nimport PopuInput from \"../../components/PopupInput.vue\";\nimport config from \"../../config\";\nimport { coverPhoto } from \"../../pages/room/coverPhoto.js\";\n\nexport default {\n  name: \"createRoom\",\n  components: { PopuInput },\n  data: function () {\n    return {\n      updateImgUrl: \"/api/file/upload\",\n      // eslint-disable-next-line no-undef\n      imgBackground: coverPhoto[Math.floor(Math.random() * 6)],\n      // eslint-disable-next-line no-undef\n      imgIcon: require(\"../../assets/live/title-icon-upload.png\"),\n      radio: \"0\",\n      roomName: \"\",\n      btoken: btoken,\n      passWord: \"\",\n    };\n  },\n  props: {\n    videoNode: Object,\n  },\n  methods: {\n    back: function () {\n      this.$router.go(-1);\n    },\n    beforeAvatarUpload: function () {\n      console.log(\"上传前\");\n    },\n    handleAvatarSuccess: function (e) {\n      console.log(\"头像上传成功回调\", e, config.picPath);\n      this.$data.imgBackground = config.picPath + e.data;\n    },\n    clickYes: function (v) {\n      console.log(\"数字上报\", v);\n      this.$data.passWord = v;\n      this.startShow();\n    },\n    preStart: function () {\n      // parmCheck();\n      if (this.$data.roomName == \"\") {\n        this.$message.error(\"房间名称不能为空\");\n        return;\n      }\n      if (this.$data.radio == \"1\") {\n        this.$refs.PopuInput.setPopup({\n          type: \"number\",\n          title: \"请输入 4 位数字密码\",\n          placeholder: \"请输入 4 位数字密码\",\n        });\n      } else {\n        this.startShow();\n      }\n    },\n    startShow: function () {\n      let data = {\n        name: this.$data.roomName,\n        isPrivate: this.$data.radio,\n        themePictureUrl: this.$data.imgBackground,\n        backgroundUrl: \"\",\n        roomType: 3, //直播\n      };\n      if (this.$data.radio == \"1\") {\n        data.password = this.$data.passWord;\n      }\n      console.log(\"config.picPath\", config.picPath, this.$data.imgBackground);\n      Request.creatLiveRoom(data).then(async (res) => {\n        if (res.data.code != 10000) {\n          // console.log(res.data.msg);\n          this.$store.dispatch(\"showToast\", {\n            value: res.data.msg,\n          });\n          this.$emit(\"startShow\", res.data);\n        } else {\n          console.log(\"创建成功\", res.data);\n          Request.joinRoom(res.data.data.roomId);\n          this.$emit(\"startShow\", res.data);\n        }\n      });\n    },\n  },\n};\n</script>\n<style scoped>\n.createPanel {\n  position: relative;\n  font-size: 0px;\n  border-radius: 0px 0px 20px 20px;\n  width: 375px;\n  height: 172px;\n  background: rgba(3, 6, 47, 0.2);\n  color: #fff;\n}\n.backIcon {\n  position: absolute;\n  background: url(\"../../assets/live/back-icon.png\") 100% 100%;\n  left: 10px;\n  top: 30px;\n  background-size: 24px 24px;\n  background-repeat: no-repeat;\n  width: 24px;\n  height: 24px;\n}\n.createContainer {\n  position: absolute;\n  top: 76px;\n  left: 24px;\n  width: 330px;\n  height: 100px;\n}\n.title {\n  position: absolute;\n  top: 0px;\n  height: 20px;\n}\n.inputer {\n  height: 40px;\n  width: 200px;\n  padding: 0px 16px;\n  background: none;\n  top: 0px;\n  left: 92px;\n  font-size: 14px;\n  color: #858585;\n  position: absolute;\n  border: #333 1px solid;\n  border-radius: 20px;\n  line-height: 40px;\n}\n.titleIcon {\n  position: absolute;\n\n  margin-top: 0px;\n}\n.avaterIcon {\n  position: absolute;\n  width: 24px;\n  height: 24px;\n  bottom: -72px;\n  right: -72px;\n}\n.avatar {\n  width: 72px;\n  height: 72px;\n  border-radius: 12px;\n  position: absolute;\n}\n.el-upload .el-upload--text {\n  width: 111px !important;\n}\n.check {\n  position: absolute;\n  height: 0px;\n  top: 52px;\n  left: 95px;\n}\n.startBtn {\n  width: 291px;\n  height: 50px;\n  line-height: 50px;\n  background: linear-gradient(90deg, #505dff 8.65%, #e92b88 100%);\n  position: fixed;\n  bottom: 32px;\n  font-size: 17px;\n  text-align: center;\n  color: #fff;\n  border-radius: 100px;\n  margin-left: calc(28.125vh - 145.5px);\n}\n</style>"],"sourceRoot":"src/components/baseUi"}]}